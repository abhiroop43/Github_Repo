                            <a href="../VehicleMaintenance/VehicleMaintenanceRequest_ALL.aspx"><i class="menu-icon fa fa-tint"></i><span class="mm-text">Vehicle Maintenance Request</span></a>
                        </li>
                    </ul>
                    <!-- / .navigation -->
                </div>
                <!-- / #main-menu-inner -->
            </div>
            <!-- / #main-menu -->
