                            <%--<img src="../assets/demo/avatars/1.jpg" alt=""/>--%>

                            <%--<a href="#" class="close">&times;</a>--%>
                        </div>
                    </div>
                    <ul class="navigation">
                        <li>
                            <a href="../Welcome.aspx"><i class="menu-icon fa fa-home"></i><span class="mm-text">Home</span></a>
                        </li>
                        <%--<li>
                            <a href="../Home.aspx"><i class="menu-icon fa fa-dashboard"></i><span class="mm-text">Dashboard</span></a>
                        </li>--%>
                        <% if (Session["IsAdminUser"].ToString().ToUpper() == "TRUE") %>
                        <% { %>
                        <li>
                            <a href="../TransportRequest/VehicleMaster.aspx"><i class="menu-icon fa fa-plus"></i><span class="mm-text">Vehicle Master</span></a>
                        </li>
                        <li>
                            <a href="../TransportRequest/VehicleUpdate.aspx"><i class="menu-icon fa fa-refresh"></i><span class="mm-text">Vehicle Update</span></a>
                        </li>
                        <% } %>
