                                            <span><%= CurrentUser %></span>
                                        </a>
                                        <ul class="dropdown-menu">
                                            <li><a href="pages-signin.html"><i class="dropdown-icon fa fa-power-off"></i>&nbsp;&nbsp;Log Out</a></li>
                                        </ul>
                                    </li>
                                </ul>
                                <!-- / .navbar-nav -->
                            </div>
                            <!-- / .right -->
                        </div>

                    </div>
                    <!-- / #main-navbar-collapse -->

                </div>
                <!-- / .navbar-inner -->
            </div>
